@{
    ViewBag.Title = "detail";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width-device-width" />
    <title>Detail Produk</title>
</head>

<body>
    <div>
        <h1>Detail Produk</h1>
        <table>
            <tr>
                <td>ID</td>
                <td>: @ViewBag.product.Id</td>
            </tr>
            <tr>
                <td>Nama</td>
                <td>: @ViewBag.product.Name</td>
            </tr>
            <tr>
                <td>Harga</td>
                <td>: @ViewBag.product.Price</td>
            </tr>
            <tr>
                <td>Stok</td>
                <td>: @ViewBag.product.Stock</td>
            </tr>
        </table>

        @Html.ActionLink("Kembali", "../Barang")
    </div>
</body>

</html>
